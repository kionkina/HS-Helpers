<script>

var url= document.getElementById("url")
console.log(url)

$.ajax({
  type: "POST",
  url: "util/ajax_helper.py",
  dataType: "json",
  data: {"url": url}
 success: function(response) {
        var div = document.getElementbyId("div1");
        div.innerHTML += response;
    }
})


</script>


